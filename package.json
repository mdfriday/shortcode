{
  "name": "@mdfriday/shortcode-compiler",
  "version": "0.2.4",
  "description": "A shortcode compiler for markdown content, supporting function and template shortcodes, nested shortcodes and frontmatter",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "markdown",
    "shortcode",
    "compiler",
    "obsidian",
    "frontmatter",
    "parser"
  ],
  "author": "sunwei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunwei/mdfriday.git"
  },
  "bugs": {
    "url": "https://github.com/sunwei/mdfriday/issues"
  },
  "homepage": "https://github.com/sunwei/mdfriday#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.9",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@mdfriday/text-template": "^0.1.3"
  }
}
